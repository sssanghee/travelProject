<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.travel.mapper.BoardMapper">
	<select id="getIdx" parameterType="String" resultType="Integer">
		SELECT COUNT(personno)
		FROM board
		WHERE id = #{id}
	</select>
    
    <insert id="insertBoard" parameterType="com.travel.model.Board">
    	INSERT INTO board(id, personno, title, content, regidate)
    	VALUES(#{id}, #{personno}, #{title}, #{content}, #{regidate})
    </insert>
    
    
    
    
</mapper>
